    <script>


        $(document).on('click', '.select2-selection__rendered', function () {
            $('.select2-search__field')[0].focus();
        })

        /* check validate user name */
        const check_validate_user_name = (user_name) => {
            if (user_name) {
                const regex = /^[A-Za-z]{1}[A-Za-z0-9_]{2,31}$/;
                return regex.test(user_name);
            } else {
                return false;
            }
        }

        /* check validate password */
        const check_validate_password = (password) => {
            if (password) {
                const regex = /^(?=.*\d)(?=.*[a-z])(?=.*[A-Z])[a-zA-Z0-9]{1,100}$/;
                return regex.test(password);
            } else {
                return false;
            }
        }
        /* check validate phone number */
        const check_validate_phone_number = (phoneNumber) => {
            if (phoneNumber) {
                const regex = /^(\+972|0)[\-]?(([23489]{1}\d{7})|[5]{1}\d{8})$/;
                return regex.test(phoneNumber);
            } else {
                return false;
            }
        }

        /* check validate email */
        const check_validate_email = (email) => {
            if (email && typeof email === "string") {
                const emailRegex =
                    /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
                return emailRegex.test(email);
            } else {
                return false;
            }
        };

        /* get data from inputs and select start */
        $.fn.getForm2obj = function () {
            var _ = {};
            $.map(this.serializeArray(), function (n) {
                const keys = n.name.match(/[a-zA-Z0-9_]+|(?=\[\])/g);
                if (keys.length > 1) {
                    let tmp = _;
                    pop = keys.pop();
                    for (let i = 0; i < keys.length, j = keys[i]; i++) {
                        tmp[j] = (!tmp[j] ? (pop == '') ? [] : {} : tmp[j]), tmp = tmp[j];
                    }
                    if (pop == '') tmp = (!Array.isArray(tmp) ? [] : tmp), tmp.push(n.value);
                    else tmp[pop] = n.value;
                } else _[keys.pop()] = n.value;
            });
            return _;
        }
        /* get data from inputs and select end */

        ///////////// make the input just accept numbers ///////////////
        $(document).on('input', '.input_number', function () {
            const value = $(this).val();
            const new_valie = value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');
            $(this).val(new_valie);
        })
        ////////////////////////////////////////////////////////////////

        /* checkbox on change */
        $(document).on('click', '.realestate-house-checkbox', function () {
            const is_checked = $(this).prop('checked');
            if (is_checked == true) {
                $(this).parent().find('.checkbox_hidden_input').val(1).trigger('change');
            } else {
                $(this).parent().find('.checkbox_hidden_input').val(0).trigger('change');
            }
        })

        const booking_from_a_to_z_validate_input_data = (for_one_element, with_out_update_html = false) => {
            let status = false;
            if (for_one_element) {
                $(for_one_element).each(function () {
                    let label = $(this).parent().find('label').text()
                    const check_is_disabled = Boolean($(this).prop('disabled'));
                    const value = $(this).val();
                    const is_required = $(this).attr('booking-from-a-to-z-form-val-is-required');
                    const is_selectpicker = $(this).attr('booking-from-a-to-z-form-val-is-selectpicker');
                    const max_length = Number($(this).attr("booking-from-a-to-z-form-val-max-length"));
                    const max_number = Number($(this).attr("booking-from-a-to-z-form-val-max-number"));
                    const min_number = Number($(this).attr("booking-from-a-to-z-form-val-min-number"));
                    const validate_facebook_link = $(this).attr('booking-from-a-to-z-form-validate_facebook_link');
                    const validate_instagram_link = $(this).attr('booking-from-a-to-z-form-validate_instagram_link');
                    const validate_youtube_link = $(this).attr('booking-from-a-to-z-form-validate_youtube_link');
                    const validate_tiktok_link = $(this).attr('booking-from-a-to-z-form-validate_tiktok_link');
                    const validate_email = $(this).attr("booking-from-a-to-z-form-val-validate-email");
                    const validate_phone = $(this).attr("booking-from-a-to-z-form-val-validate-phone");
                    const validate_user_name = $(this).attr("booking-from-a-to-z-form-val-validate-user-name");
                    const validate_password = $(this).attr("booking-from-a-to-z-form-val-validate-password");
                    const validate_confirm_password = $(this).attr("booking-from-a-to-z-form-val-validate-confirm-password");
                    if (!label) {
                        label = $(this).attr('placeholder');
                    }
                    if (check_is_disabled == true) {
                        /* remove error */
                        if ($(this).data('select2')) {
                            $(this).next().removeClass('has-error');
                            $(this).parent().find('.input_error_message').css('visibility', 'hidden');
                            $(this).parent().find('.input_error_message').text("")
                        } else if (is_selectpicker == 'true') {
                            $(this).next().removeClass('has-error');
                            $(this).parent().parent().find('.input_error_message').css('visibility', 'hidden');
                            $(this).parent().parent().find('.input_error_message').text("")
                        } else {
                            $(this).removeClass('has-error');
                            $(this).parent().find('.input_error_message').css('visibility', 'hidden');
                            $(this).parent().find('.input_error_message').text("")
                        }

                        status = true;
                        return;
                    }

                    /* Check Required */
                    if (is_required == 'true' && (!value || value.length == 0)) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} Is Required`

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                                $(this).parent().find('.input_error_message').text(error_message)
                            } else if (is_selectpicker == 'true') {
                                $(this).next().addClass('has-error');
                                $(this).parent().parent().find('.input_error_message').css('visibility', 'visible');
                                $(this).parent().parent().find('.input_error_message').text(error_message)
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                                $(this).parent().find('.input_error_message').text(error_message)
                            }
                        }
                        return;
                    }

                    /* check max length */
                    if (max_length > 0 && value.length > max_length) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} Maximum ${max_length} Letters`
                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }


                    /* check max number */
                    if (max_number != undefined && value > max_number) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} Maximum ${max_number}`
                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }

                    /* check min number */
                    if (min_number != undefined && value < min_number) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} Minimum ${min_number}`
                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }

                    /* check validate email */
                    if (validate_email == 'true' && value && !check_validate_email(value)) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} Please enter valid email`

                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }

                    /* check validate phone */
                    if (validate_phone == 'true' && value && !check_validate_phone_number(value)) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} there is an error in the phone number`
                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }
                    /* check validate user name */
                    if (validate_user_name == 'true' && value && !check_validate_user_name(value)) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `${label} user name validation error`
                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }

                    /* check validate password */
                    if (validate_password == 'true' && value && !check_validate_password(value)) {
                        if (with_out_update_html == false) {
                            /* update error message */
                            const error_message = `Password must be between 1 to 100 characters long, and include at least one uppercase letter, one lowercase letter, and one number. No special characters are allowed.`
                            $(this).parent().find('.input_error_message').text(error_message)

                            if ($(this).data('select2')) {
                                $(this).next().addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            } else {
                                $(this).addClass('has-error');
                                $(this).parent().find('.input_error_message').css('visibility', 'visible');
                            }
                        }
                        return;
                    }

                    /* check validate confirm password */
                    if (validate_confirm_password == 'true') {
                        const password = $(this).parent().parent().find("input[booking-from-a-to-z-form-val-validate-password='true']").val();

                        if (value != password) {
                            if (with_out_update_html == false) {
                                /* update error message */
                                const error_message = `${label} confirm password validation error`
                                $(this).parent().find('.input_error_message').text(error_message)

                                if ($(this).data('select2')) {
                                    $(this).next().addClass('has-error');
                                    $(this).parent().find('.input_error_message').css('visibility', 'visible');
                                } else {
                                    $(this).addClass('has-error');
                                    $(this).parent().find('.input_error_message').css('visibility', 'visible');
                                }
                            }
                            return;
                        }
                    }
                    /* remove error */
                    if ($(this).data('select2')) {
                        $(this).next().removeClass('has-error');
                        $(this).parent().find('.input_error_message').css('visibility', 'hidden');
                        $(this).parent().find('.input_error_message').text("")
                    } else if (is_selectpicker == 'true') {
                        $(this).next().removeClass('has-error');
                        $(this).parent().parent().find('.input_error_message').css('visibility', 'hidden');
                        $(this).parent().parent().find('.input_error_message').text('')
                    } else {
                        $(this).removeClass('has-error');
                        $(this).parent().find('.input_error_message').css('visibility', 'hidden');
                        $(this).parent().find('.input_error_message').text("")
                    }

                    status = true;
                    return;
                })

                return status;
            } else {
                return status;
            }
        }

        /////////////////// select file ////////////////////
        $(document).on("change", "#pm_upload_file_upload_file", function (element) {
            var extension = $(this).val().split(".").pop().toLowerCase();
            if ($.inArray(extension, ["gif", "png", "jpg", "jpeg", 'pdf']) == -1) {
                $(".pm_upload_file_error_msg").text("Not an Image...");
            } else {
                $(".pm_upload_file_error_msg").text("");
                $(".pm_upload_file_button_outer").addClass("pm_upload_file_file_uploading");
                setTimeout(function () {
                    $(".pm_upload_file_button_outer").addClass("pm_upload_file_file_uploaded");
                }, 3000);
                var uploadedFile = URL.createObjectURL(element.target.files[0]);
                setTimeout(function () {
                    if (extension != 'pdf') {
                        $("#pm_upload_file_uploaded_view")
                            .append('<img src="' + uploadedFile + '" />')
                            .addClass("show");
                    }
                }, 3500);
            }
        });

        $(document).on("click", ".pm_upload_file_file_remove", function () {
            $("#pm_upload_file_uploaded_view").removeClass("show");
            $("#pm_upload_file_uploaded_view").find("img").remove();
            $(".pm_upload_file_button_outer").removeClass("pm_upload_file_file_uploading");
            $(".pm_upload_file_button_outer").removeClass("pm_upload_file_file_uploaded");
            $("#pm_upload_file_upload_file").val('');
        });
        /////////////////////////////////////////////////////

        $(document).ready(function () {

            $('.datatable').DataTable({
                processing: true,
                serverSide: false,
                responsive: true,
                aLengthMenu: [
                    [25, 50, 100, 200, -1],
                    [25, 50, 100, 200, "All"]
                ],
            });

            /* check select validate on change */
            $(document).on('change', 'select , input', function () {
                const check_if_this_with_validate = Boolean($(this).attr('with-booking-from-a-to-z-validate'));
                if (check_if_this_with_validate == true) {
                    booking_from_a_to_z_validate_input_data(this);
                }
            })

            /* check input or textarea validate on input */
            $(document).on('input', 'input , textarea', function () {
                const check_if_this_with_validate = Boolean($(this).attr('with-booking-from-a-to-z-validate'));
                if (check_if_this_with_validate == true) {
                    booking_from_a_to_z_validate_input_data(this);
                }
            })

            $(document).on('focus', 'input', function (event) {
                if ($(this).data('daterangepicker')) {
                    event.preventDefault();
                    this.blur();
                }
            })
        })
    </script>


